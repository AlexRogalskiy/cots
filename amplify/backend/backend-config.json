{
	"hosting": {
		"amplifyhosting": {
			"service": "amplifyhosting",
			"type": "cicd"
		}
	},
	"auth": {
		"userPoolGroups": {
			"service": "Cognito-UserPool-Groups",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "cots",
					"attributes": [
						"UserPoolId",
						"AppClientIDWeb",
						"AppClientID",
						"IdentityPoolId"
					]
				}
			]
		},
		"cots": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "cotsPostConfirmation",
					"triggerProvider": "Cognito",
					"attributes": [
						"Arn",
						"Name"
					]
				}
			]
		}
	},
	"function": {
		"cotsPostConfirmation": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda"
		}
	},
	"api": {
		"cots": {
			"service": "AppSync",
			"providerPlugin": "awscloudformation",
			"output": {
				"authConfig": {
					"additionalAuthenticationProviders": [
						{
							"authenticationType": "API_KEY",
							"apiKeyConfig": {
								"description": "cots",
								"apiKeyExpirationDays": "365"
							}
						}
					],
					"defaultAuthentication": {
						"authenticationType": "AMAZON_COGNITO_USER_POOLS",
						"userPoolConfig": {
							"userPoolId": "authcots"
						}
					}
				}
			}
		}
	}
}
